{
  "name": "scaffolding-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext \"**/*.{ts,tsx,vue}\" --no-error-on-unmatched-pattern ",
    "lint:fix": "eslint src --ext \"**/*.{ts,tsx,vue}\" --fix --no-error-on-unmatched-pattern ",
    "format": "npx prettier \"src/**/*.{js,jsx,ts,tsx,html,css,scss,vue}\" --write",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "husky": {
    "hooks": {
      "applypatch-msg": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@denisibanez/design-system-ui": "^0.1.9",
    "@pinia/testing": "^0.0.15",
    "@quasar/babel-preset-app": "^2.0.2",
    "@quasar/extras": "^1.15.11",
    "axios": "^1.3.4",
    "pinia": "^2.0.33",
    "quasar": "^2.11.8",
    "sass": "1.32.12",
    "vue": "^3.2.47",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.3.0",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.7",
    "@vue/test-utils": "^2.3.2",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": ">=10",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.29.7",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
